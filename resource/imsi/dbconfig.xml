<?xml version="1.0" encoding="UTF-8"?>

<root>
	<procedure id="employee_list">
		<name>Sample@listEmployeeBy(:deptno)</name>
		<params>
			<param name="deptno" type="Types.INTEGER" />
		</params>

	</procedure>

	<procedure id="employee_add">
		<name>Sample@addEmployeeWith(:empno,:ename,:job,:mgr,:hiredate,:sal,:comm,:deptno)</name>
		<params>
			<param name="empno" type="Types.INTEGER" />
			<param name="ename" type="Types.VARCHAR" />
			<param name="job" type="Types.VARCHAR" />
			<param name="mgr" type="Types.INTEGER" />
			<param name="hiredate" type="Types.VARCHAR" />
			<param name="sal" type="Types.INTEGER" />
			<param name="comm" type="Types.INTEGER" />
			<param name="deptno" type="Types.INTEGER" />
		</params>
	</procedure>

</root>
